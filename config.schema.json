{
  "pluginAlias": "homebridge-plugin-ui",
  "pluginType": "platform",
  "singular": true,
  "schema": {
    "type": "object",
  "properties": {
    "username": {
      "title": "Benutzername",
      "type": "string",
      "required": true,
      "default": "default-username"
    },
    "password": {
      "title": "Passwort",
      "type": "string",
      "required": true,
      "default": "default-password"
    },
    "address": {
      "title": "Serveradresse",
      "type": "string",
      "required": true,
      "default": "http://default-server-address"
    }
  }
}
